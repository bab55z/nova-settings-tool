{
    "name": "bab55z/nova-settings-tool",
    "description": "Settings for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "unisharp/laravel-settings": "^2.0",
        "spatie/laravel-translatable": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Bab55z\\NovaSettingsTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bab55z\\NovaSettingsTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
